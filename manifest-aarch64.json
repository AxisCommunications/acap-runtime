{
    "schemaVersion": "1.1",
    "acapPackageConf": {
        "setup": {
            "friendlyName": "ACAP Runtime",
            "appName": "acapruntime",
            "vendor": "Axis Communications",
            "embeddedSdkVersion": "3.0",
            "user": {
                "group": "sdk",
                "username": "sdk"
            },
            "vendorUrl": "https://www.axis.com",
            "runOptions": "-o",
            "runMode": "once",
            "version": "1.2.0"
        },
        "configuration": {
            "paramConfig": [
                {
                    "name": "Verbose",
                    "default": "no",
                    "type": "enum:no|No, yes|Yes"
                },
                {
                    "name": "ChipId",
                    "default": "12",
                    "type": "string"
                },
                {
                    "name": "IpPort",
                    "default": "0",
                    "type": "string"
                },
                {
                    "name": "UseTLS",
                    "default": "yes",
                    "type": "enum:no|No, yes|Yes"
                }
            ]
        }
    }
}
